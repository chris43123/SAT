@model IEnumerable<SAT.Models.tbJornadas>

@{
    ViewBag.Title = "Listado jornadas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="x_panel">
	<div class="x_title">
		<h2>Listado de jornadas</h2>
		<div class="nav navbar-right panel_toolbox">
			@Html.ActionLink("Resgitrar jornadas", "Create", null, new {@class ="btn btn-info"})
		</div>
		<div class="clearfix"></div>
	</div>
<table class="table table-striped table-bordered dataTable" id="TableGrid">
<thead>
	<tr>
        <th>
            @Html.DisplayNameFor(model => model.jor_Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jor_UsuarioCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jor_FechaCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jor_UsuarioModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jor_FechaModifica)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.jor_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jor_UsuarioCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jor_FechaCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jor_UsuarioModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jor_FechaModifica)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.jor_Id }, new {@class = "btn btn-default btn-xs"})
            @Html.ActionLink("Detalle", "Details", new { id=item.jor_Id }, new {@class = "btn btn-default btn-xs"})
        </td>
    </tr>
}
</tbody>
</table>
</div>
