@model IEnumerable<SAT.Models.tbMunicipios>

<table class="table table-striped table-bordered"  id="tlb">
<thead>
	<tr>
        <th>
            @Html.DisplayNameFor(model => model.mun_Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mun_FechaCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mun_FechaModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbUsuarios.usu_NombreUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbUsuarios1.usu_NombreUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbDepartamentos.dep_Descripcion)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.mun_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mun_FechaCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mun_FechaModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUsuarios.usu_NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUsuarios1.usu_NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbDepartamentos.dep_Descripcion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.mun_Id }, new {@class = "btn btn-default btn-xs"})
            @Html.ActionLink("Detalle", "Details", new { id=item.mun_Id }, new {@class = "btn btn-default btn-xs"})
        </td>
    </tr>
  
}
</tbody>

</table>
</div>
